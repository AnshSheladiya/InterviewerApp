    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon backend/server.js",
    "seeds": "node backend/seeds/seeds.js",
    "prettier": "prettier --write \"**/*.js\"",
    "editorconfig": "npx editorconfig-checker",
    "lint": "eslint . --config backend/test/.eslintrc.json",
    "lint:fix": "eslint . --fix --config backend/test/.eslintrc.json",
    "frontend-install": "cd frontend && npm install --force",
    "install-all": "npm install && npm run frontend-install",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm start", 
    "start:backend": "npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "postbuild": "npm install --prefix frontend && npm run build --prefix frontend",
    "production": "npm run install-all && npm run postbuild ",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "minify": "terser backend/src/app.js -o app.min.js -c terser.config.js"
  },
